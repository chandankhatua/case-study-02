{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-cs2-grp3"
		},
		"LinkedServiceAzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedServiceAzureBlobStorage'"
		},
		"LinkedServiceAzureDatabricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1005-053853-tj9rkb84"
		},
		"LinkedServiceAzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-cs2-grp3-v2.vault.azure.net/"
		},
		"LinkedServiceHttpServer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/manojkumarsingh77/Shell2023/main/EVCarbonEmission/DataSets/CO2%20Emissions_Canada.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-cs2-grp3_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkedServiceAzureBlobStorage_connectionString": {
						"value": "[parameters('LinkedServiceAzureBlobStorage_connectionString')]"
					},
					"LinkedServiceAzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LinkedServiceAzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"LinkedServiceHttpServer_properties_typeProperties_url": {
						"value": "[parameters('LinkedServiceHttpServer_properties_typeProperties_url')]"
					},
					"LinkedServiceAzureDatabricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LinkedServiceAzureDatabricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}